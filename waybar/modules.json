{
  "workspaces": { "format": "{name}" },
  "window": { "max-length": 50 },
  "clock": { "format": "{:%I:%M %p}" },
  "pulseaudio": { "on-click": "pavucontrol" },
  "network": { "on-click": "GTK_THEME=Adwaita:dark nmgui
" },
  "tray": {},
